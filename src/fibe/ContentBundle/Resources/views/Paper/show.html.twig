{% extends '::base.html.twig' %}

{% block body -%}
  <h1>Paper</h1>

  <table class="record_properties">
    <tbody>
    <tr>
      <th>Id</th>
      <td>{{ entity.id }}</td>
    </tr>
    <tr>
      <th>Label</th>
      <td>{{ entity.label }}</td>
    </tr>
    <tr>
      <th>Abstract</th>
      <td>{{ entity.abstract }}</td>
    </tr>
    <tr>
      <th>Url</th>
      <td>{{ entity.url }}</td>
    </tr>
    <tr>
      <th>Publisher</th>
      <td>{{ entity.publisher }}</td>
    </tr>
    <tr>
      <th>Publishdate</th>
      <td>{{ entity.publishDate }}</td>
    </tr>
    <tr>
      <th>Slug</th>
      <td>{{ entity.slug }}</td>
    </tr>
    </tbody>
  </table>

  <ul class="record_actions">
    <li>
      <a href="{{ path('paper') }}">
        Back to the list
      </a>
    </li>
    <li>
      <a href="{{ path('paper_edit', { 'id': entity.id }) }}">
        Edit
      </a>
    </li>
    <li>{{ form(delete_form) }}</li>
  </ul>
{% endblock %}
