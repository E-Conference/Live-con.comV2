{% extends '::base.html.twig' %}

{% block body -%}
  <h1>SocialServiceAccount list</h1>

  <table class="records_list">
    <thead>
    <tr>
      <th>Id</th>
      <th>Accountname</th>
      <th>Socialservice</th>
      <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
      <tr>
        <td><a href="{{ path('socialserviceaccount_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
        <td>{{ entity.accountName }}</td>
        <td>{{ entity.socialService }}</td>
        <td>
          <ul>
            <li>
              <a href="{{ path('socialserviceaccount_show', { 'id': entity.id }) }}">show</a>
            </li>
            <li>
              <a href="{{ path('socialserviceaccount_edit', { 'id': entity.id }) }}">edit</a>
            </li>
          </ul>
        </td>
      </tr>
    {% endfor %}
    </tbody>
  </table>

  <ul>
    <li>
      <a href="{{ path('socialserviceaccount_new') }}">
        Create a new entry
      </a>
    </li>
  </ul>
{% endblock %}
